{
  "require": {
    "php": "^8.3",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "ext-simplexml": "*",
    "aura/sql": "^5.0",
    "guzzlehttp/guzzle": "^7.9",
    "laminas/laminas-coding-standard": "^2.5",
    "laminas/laminas-config-aggregator": "^1.15",
    "laminas/laminas-servicemanager": "^4.2",
    "league/flysystem": "^3.29",
    "league/flysystem-aws-s3-v3": "^3.0",
    "ramsey/uuid": "^4.7",
    "symfony/console": "^7.1",
    "symfony/process": "^7.1",
    "symfony/var-dumper": "^7.1",
    "thecodingmachine/safe": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "AspirePress\\AspireSync\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AspirePress\\AspireSync\\Tests\\Unit\\": "./tests/unit",
      "AspirePress\\AspireSync\\Tests\\Functional\\": "./tests/functional",
      "AspirePress\\AspireSync\\Tests\\Helpers\\": "./tests/helpers"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.12",
    "phpunit/phpunit": "^11.3",
    "roave/security-advisories": "dev-latest",
    "thecodingmachine/phpstan-safe-rule": "^1.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
